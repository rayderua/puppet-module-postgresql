# ---------------------------------------------------
# MAPNAME       SYSTEM-USERNAME         PG-USERNAME
# ---------------------------------------------------

<% @default_pg_ident.map do | v | -%>
<% if v.is_a?(Hash) -%>
<%= v['mapname'] %>  <%= v['system'] %> <%= v['username'] %>
<%   end %>
<% end %>

<% @pg_ident.map do | v | -%>
<% if v.is_a?(Hash) -%>
<%= v['mapname'] %>  <%= v['system'] %> <%= v['username'] %>
<%   end %>
<% end %>

