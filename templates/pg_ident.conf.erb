#
# HEADER: Managet by puppet
#
# ---------------------------------------------------
# MAPNAME       SYSTEM-USERNAME         PG-USERNAME
# ---------------------------------------------------
<% if @default_pg_ident.is_a?(Array) -%>
<% @default_pg_ident.map do | v | -%>
<% if v.is_a?(Hash) -%>
<%= v['mapname'] %>  <%= v['system'] %> <%= v['username'] %>
<% end -%>
<% end -%>
<% end -%>

<% if @pg_ident.is_a?(Array) -%>
<% @pg_ident.map do | v | -%>
<% if v.is_a?(Hash) -%>
<%= v['mapname'] %>  <%= v['system'] %> <%= v['username'] %>
<% end -%>
<% end -%>
<% end -%>
