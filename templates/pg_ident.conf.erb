#
# HEADER: Managet by puppet
#
# ---------------------------------------------------
# MAPNAME       SYSTEM-USERNAME         PG-USERNAME
# ---------------------------------------------------
<% if @default_ident.is_a?(Array) -%>
<% @default_ident.map do | v | -%>
<% if v.is_a?(Hash) -%>
<% if !v['mapname'].nil? -%><%= "%-32s" % v['mapname'] %><% end -%> <% if !v['system'].nil? -%><%= "%-32s" % v['system'] %><% end -%> <% if !v['username'].nil? -%><%= "%-32s" % v['username'] %><% end -%>
<% end -%>
<% end -%>
<% end -%>

<% if @pg_ident.is_a?(Array) -%>
<% @pg_ident.map do | v | -%>
<% if v.is_a?(Hash) -%>
<% if !v['mapname'].nil? -%><%= "%-32s" % v['mapname'] %><% end -%> <% if !v['system'].nil? -%><%= "%-32s" % v['system'] %><% end -%> <% if !v['username'].nil? -%><%= "%-32s" % v['username'] %><% end -%>
<% end -%>
<% end -%>
<% end -%>
